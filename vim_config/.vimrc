set tabstop=4
set autoindent
set expandtab
set ruler
set nocompatible
set backspace=2
set hlsearch
set nu
let &t_Co=256
set laststatus=2
set statusline=%f
set clipboard=unnamed
syntax on
filetype plugin on
color seoul256 
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
let g:netrw_liststyle=3
